@model IEnumerable<Paciente>

<table class="table">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Correo</th>
            <th>Fecha De Nacimiento</th>
            <th>Teléfono</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var paciente in Model)
        {
            <tr>
                <td>@paciente.Nombre</td>
                <td>@paciente.FechaNacimiento</td>
                <td>@paciente.Correo</td>
                <td>@paciente.Telefono</td>
                <td>
                    @Html.ActionLink("Editar", "Edit", new { id = paciente.Id_Paciente }, new { @class = "btn btn-primary" })
                </td>
            </tr>
        }
    </tbody>
</table>
